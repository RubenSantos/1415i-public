
<h2><%= objectTitle %></h2>
<table class="table table-condensed table-hover">
  <thead>
    <tr>
      <th>name</th>
      <th>value</th>
    </tr>
  </thead>
  <tbody>
<% for(var name in objectValue) { %>
  <tr>
    <td><%= name %></td> 
    <td><%= objectValue[name] %></td>
  </tr>
<% } %>
  </tbody>
</table>